// C++ code
// https://www.tinkercad.com/things/45gcEHPJF9K-delete-this/editel?sharecode=PQBmInAXqd-DmiCrdJZojTBZjT1glnvxLlndNVAmGEg
// LEDs on 2, 3, 4, 5, 6
void setup()
{
  
pinMode(2, OUTPUT);
pinMode(3, OUTPUT);
pinMode(4, OUTPUT);
pinMode(5, OUTPUT);
pinMode(6, OUTPUT);

  
}

void loop()
{
int letterDelay = 500;  
int wipeDelay = 2000; 
digitalWrite(2, HIGH);
delay(letterDelay); 
digitalWrite(3, HIGH);
delay(letterDelay); 
digitalWrite(4, HIGH);
delay(letterDelay); 
digitalWrite(5, HIGH);
delay(letterDelay);   
digitalWrite(6, HIGH);
delay(letterDelay); 
digitalWrite(2, LOW);
digitalWrite(3, LOW);
digitalWrite(4, LOW);
digitalWrite(5, LOW);
digitalWrite(6, LOW);
delay(letterDelay); 
}


